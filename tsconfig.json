        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false
    },
    "exclude": [
	    "services.schools/node_modules",
        "services.schools/typings",
        "ui/node_modules",
        "ui/typings",
        "services.schools.example"
    ],
